#include <bits/stdc++.h>
using namespace std;
typedef long long ll;
typedef pair<int, int> pii;
const int MAX = INT_MAX;

int N;
vector<double>v;

int main()
{
    ios_base::sync_with_stdio(0); 
	cin.tie(0);

    cin >> N;

    for(int i=0; i<N; i++) 
    {
        double n;
        cin >> n;
        v.push_back(n);
    }

    sort(v.begin(), v.end());

    cout << fixed; //소수점 자리 고정
    cout.precision(3); //3자리까지
    for(int i=0; i<7; i++) cout << v[i] << "\n";


    return 0;
}
