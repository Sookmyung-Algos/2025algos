#include<iostream>
#include <algorithm>
#include <numeric> 

using namespace std;
#define ll long long
ll  A, N;

ll EEA(ll a, ll b) {
    ll r1 = a, r2 = b, s1 = 1, s2 = 0, t1 = 0, t2 = 1;
    ll r, s, t, q;

    while (r2) {
        q = r1 / r2;

        r = r1 % r2;
        r1 = r2;
        r2 = r;

        s = s1 - q * s2;
        s1 = s2;
        s2 = s;

        t = t1 - q * t2;
        t1 = t2;
        t2 = t;
    }

    if (r1 != 1) return -1;

    s1 %= b;
    if (s1 < 0) s1 += b;

    return s1;
}


int main() {
	cin >> N>>A;

	ll  mul = 0, plu = N-A;
	if (plu < 0)
		plu = gcd(plu, N);
	if (gcd(A, N) != 1) {
		mul = -1;
	}

	else {
		mul = (EEA(A, N));
		if (mul < 0 && mul != -1) {
			mul = gcd(mul, N);
		}
	}

	cout << plu << " " << mul;
	return 0;
}
